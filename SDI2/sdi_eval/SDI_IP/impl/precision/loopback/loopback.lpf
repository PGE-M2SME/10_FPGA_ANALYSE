LOCATE COMP "lcd10"            SITE "E12" ;

# ------------ IO buffer settings
# DIP Switches
IOBUF PORT "hd_sdn_in"         IO_TYPE=LVCMOS18 ;
IOBUF PORT "tg_hdn_in"         IO_TYPE=LVCMOS18 ;
IOBUF PORT "sw1_3"             IO_TYPE=LVCMOS18 ; # lb_lan_in
#IOBUF PORT "sw1_4"            IO_TYPE=LVCMOS18 ; # use if SD_LDR
IOBUF PORT "pg_std_num(0)"     IO_TYPE=LVCMOS18 ;
IOBUF PORT "pg_std_num(1)"     IO_TYPE=LVCMOS18 ;
IOBUF PORT "pg_std_num(2)"     IO_TYPE=LVCMOS18 ;
IOBUF PORT "pg_std_num(3)"     IO_TYPE=LVCMOS18 ;
IOBUF PORT "pg_std_num(4)"     IO_TYPE=LVCMOS18 ;
